                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
              </rowConstraints>
               <children>
                  <TextField fx:id="nome" promptText="Nome" GridPane.columnIndex="1" />
                  <TextField fx:id="foto" promptText="Foto" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                  <Label text="Foto:" GridPane.rowIndex="1" />
                  <Label text="Delegação:" GridPane.rowIndex="2" />
                  <Label text="Nome:" />
                  <ComboBox fx:id="delegacao" prefWidth="150.0" GridPane.columnIndex="1" GridPane.rowIndex="2" />
               </children>
            </GridPane>
            <GridPane VBox.vgrow="ALWAYS">
              <columnConstraints>
                <ColumnConstraints halignment="RIGHT" hgrow="SOMETIMES" maxWidth="120.0" minWidth="10.0" prefWidth="80.0" />
                  <ColumnConstraints hgrow="SOMETIMES" maxWidth="235.0" minWidth="10.0" prefWidth="134.0" />
                  <ColumnConstraints halignment="RIGHT" hgrow="SOMETIMES" maxWidth="126.0" minWidth="10.0" prefWidth="126.0" />
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="251.0" minWidth="10.0" prefWidth="149.0" />
                  <ColumnConstraints hgrow="SOMETIMES" maxWidth="178.0" minWidth="10.0" prefWidth="49.0" />
              </columnConstraints>
              <rowConstraints>
                  <RowConstraints minHeight="10.0" prefHeight="30.0" valignment="CENTER" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" valignment="CENTER" vgrow="SOMETIMES" />
              </rowConstraints>
               <children>
                  <Label text="Altura:" GridPane.rowIndex="1" />
                  <Label text="Peso:" GridPane.columnIndex="2" GridPane.rowIndex="1" />
                  <TextField fx:id="altura" promptText="Altura" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                  <TextField fx:id="peso" promptText="Peso" GridPane.columnIndex="3" GridPane.rowIndex="1" />
                  <TextField fx:id="genero" promptText="Gênero" GridPane.columnIndex="1" />
                  <DatePicker fx:id="dataNascimento" GridPane.columnIndex="3" />
                  <Label text="Gênero:" />
                  <Label text="Data de nascimento:" GridPane.columnIndex="2" />
               </children>
            </GridPane>
         </children>
      </VBox>
   </center>
</BorderPane>
